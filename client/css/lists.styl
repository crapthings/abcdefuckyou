.list-swagga
	list-style none
	margin 0
	padding 0

.item-swagga
	margin-bottom 30px
	border 1px solid #ccc
	border-radius 4px
	& .content
		padding 13px
	& .foot
		border-top 1px solid #ccc
		padding 3px 13px
		& > ul
			list-style none
			margin 0
			padding 0
			& > li
				float left
				margin 0 10px 0 0
				padding 0
				margin-right 10px
		& > ul.pull-right
			& > li
				float left
				margin 0 0 0 10px
				padding 0

.content
	& h1
	& h2
	& h3
	& h4
	& h5
	& h6
		margin-top 0
	& embed
		width 100%
		height 400px
